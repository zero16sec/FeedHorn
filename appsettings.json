{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "FeedHorn.Services.UrlMonitoringService": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=feedhorn.db"
  },
  "MonitoringSettings": {
    "CheckIntervalMinutes": 5,
    "MaxChecksPerUrl": 100,
    "RequestTimeoutSeconds": 30
  }
}
